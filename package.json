{
  "name": "machinepack-github",
  "version": "2.2.0",
  "description": "Communicate with the Github API to get repos, commits, etc.",
  "scripts": {
    "test": "mocha -b"
  },
  "keywords": [
    "machinepack",
    "machines",
    "github",
    "github-api"
  ],
  "author": "Mike McNeil",
  "license": "MIT",
  "devDependencies": {
    "mocha": "~1.19.0"
  },
  "dependencies": {
    "github": "~0.1.16",
    "lodash": "^2.4.1",
    "machine": "^1.0.0",
    "machinepack-http": "^1.0.1"
  },
  "repository": {
    "url": "https://github.com/mikermcneil/machinepack-github.git",
    "type": "git"
  },
  "machinepack": {
    "machineDir": "machines",
    "machines": [
      "get-repo",
      "create-repo",
      "get-user-details",
      "list-organization-repos",
      "list-repo-commits-at-path",
      "list-repo-commits",
      "list-repo-activity",
      "list-repos",
      "build-installable-url",
      "get-login-url",
      "get-access-token"
    ],
    "devLinks": [
      "https://developer.github.com/v3/",
      "http://mikedeboer.github.io/node-github"
    ]
  }
}
